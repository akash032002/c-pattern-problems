This is in c++
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
  int n;
  cin>>n;
  int m=65+n-1;
  for(int i=n;i>=1;i--){

      for(char k=65;k<=m;k++){
      cout<<k;

      }cout<<"\n";
      m--;

  }

  
}


this in c++

#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
  int n;
  cin>>n;
  char m='A';
  for(int i=1;i<=n;i++){

      for(char k=1;k<=i;k++){
      cout<<m;

      }cout<<"\n";
      m++;

  }

  
}
